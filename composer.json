{
    "name": "ourenergy/wits-api",
    "description": "A wrapper for the Wholesale Information Trading System API",
    "type": "library",
    "require": {
        "ext-json": "*",
        "php-http/client-common": "^2.6",
        "php-http/discovery": "^1.14",
        "nyholm/psr7": "^1.5",
        "league/oauth2-client": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "php-http/mock-client": "^1.5",
        "php-http/guzzle7-adapter": "^1.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "OurEnergy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OurEnergy\\WitsApi\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Andrew Joll",
            "email": "andrew.joll@ourenergy.co.nz"
        }
    ],
    "minimum-stability": "stable"
}
